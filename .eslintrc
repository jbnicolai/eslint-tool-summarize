{
	"env": {
		"browser": true,
		"node": true,
		"amd": true,
		"mocha": true
	},
	"globals": {
		"_": false,
		"$": false,
		"angular": false,
		"bootbox": false,
		"JL": false,

		"expect": false,
		"inject": false,
		"module": false,
		"waitsFor": false,
		"runs": false,
		"describeAngularSpec": false
	},
	"rules": {
		"no-alert": 1,
		"no-caller": 1,
		"no-bitwise": 0,
		"no-console": 1,
		"no-comma-dangle": 1,
		"no-control-regex": 1,
		"no-debugger": 1,
		"no-dupe-keys": 1,
		"no-div-regex": 1,
		"no-else-return": 1,
		"no-empty": 1,
		"no-empty-class": 1,
		"no-eq-null": 1,
		"no-eval": 1,
		"no-ex-assign": 1,
		"no-floating-decimal": 1,
		"no-implied-eval": 1,
		"no-with": 1,
		"no-fallthrough": 1,
		"no-unreachable": 1,
		"no-undef": 1,
		"no-undef-init": 1,
		"no-octal": 1,
		"no-obj-calls": 1,
		"no-new-wrappers": 1,
		"no-new": 1,
		"no-new-func": 1,
		"no-native-reassign": 1,
		"no-plusplus": 0,
		"no-delete-var": 1,
		"no-return-assign": 1,
		"no-array-constructor": 1,
		"no-new-object": 1,
		"no-label-var": 1,
		"no-ternary": 0,
		"no-self-compare": 1,
		"no-spaced-func": 1,
		"no-sync": 0,
		"no-underscore-dangle": 0,
		"no-loop-func": 1,
		"no-empty-label": 1,
		"no-unused-vars": 0,
		"no-script-url": 1,
		"no-proto": 1,
		"no-iterator": 1,
		"no-mixed-requires": [
			1,
			false
		],
		"no-wrap-func": 1,
		"no-shadow": 1,
		"no-use-before-define": 0,
		"no-invalid-regexp": 1,
		"no-regex-spaces": 1,
		"valid-typeof": 1,
		"brace-style": "stroustrup",
		"block-scoped-var": 0,
		"camelcase": 0,
		"curly": 1,
		"dot-notation": 1,
		"eqeqeq": 1,
		"new-parens": 1,
		"guard-for-in": 0,
		"radix": 0,
		"new-cap": 1,
		"one-var": 0,
		"quotes": 0,
		"quote-props": 0,
		"semi": 1,
		"use-isnan": 1,
		"max-depth": [
			1,
			4
		],
		"max-params": [
			0,
			10
		],
		"max-statements": [
			1,
			50
		],
		"complexity": [
			1,
			15
		],
		"wrap-iife": 0,
		"wrap-regex": 0,
		"no-multi-str": 1,
		"consistent-this": [
			1,
			"self"
		],
		"max-len": [
			0,
			150,
			4
		],



		"angular-di-notation": [
			1,
			"arr",
			true
		]
	}
}